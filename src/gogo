#!/bin/bash


function gogo() {
	if [ "$1" = "gh" ]; then 
		cd $GOPATH/src/github.com
	elif [ "$1" = "me" ]; then
		cd $GOPATH/src/github.com/a-hilaly
	elif [ "$1" = "kube" ]; then
		cd $GOPATH/src/github.com/kubernetes
	elif [ "$1" = "sii" ]; then
		cd $GOPATH/src/github.com/siisxb
	elif [ "$1" = "supbank" ];then
		cd $GOPATH/src/gitlab.com/supbank
	elif [ "$1" = "kubei" ]; then
		cd $GOPATH/src/github.com/kubernetes-incubator
	else
		cd $GOPATH/src/github.com/$1
	fi
}

